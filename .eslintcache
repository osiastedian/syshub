der()\r\n   * @function\r\n   */\r\n  componentDidMount() {\r\n    this._isMounted = true;\r\n    this.apiLoader();\r\n  }\r\n\r\n  /**\r\n   * Cancel all requests\r\n   * @function\r\n   */\r\n  componentWillUnmount() {\r\n    this._isMounted = false;\r\n    this.source.cancel('Request has been canceled');\r\n  }\r\n\r\n  /**\r\n   * Fetch the governance and stats data from the API to modify the state and show the component\r\n   * @function\r\n   */\r\n  async apiLoader() {\r\n    const CancelToken = axios.CancelToken;\r\n    this.source = CancelToken.source();\r\n    let axiosConfig = {\r\n      headers: {\r\n        'Content-Type': 'application/json;charset=UTF-8',\r\n        'Accept': 'application/json, text/plain, */*',\r\n        \"Access-Control-Allow-Origin\": \"*\",\r\n      },\r\n      cancelToken: this.source.token\r\n    };\r\n\r\n    let data = await axios\r\n      .get(`${API_URI}/statsInfo/mnStats`, {\r\n        headers: {\r\n          'appclient': process.env.REACT_APP_CLIENT\r\n        },\r\n        cancelToken: this.source.token\r\n      })\r\n      .then(function (result) {\r\n        return result;\r\n      })\r\n      .catch(function (error) {\r\n        // console.log(error)\r\n      });\r\n\r\n    // let govres = await axios.post('https://syscoin.dev/govlist', [], axiosConfig)\r\n    // .then((res) => {\r\n    //     return res;\r\n    // })\r\n    // .catch((err) => {\r\n    //     // console.log(err);\r\n    // });\r\n\r\n    let govres = await axios.get(`${API_URI}/statsInfo/list`, {\r\n      headers: {\r\n        'appclient': process.env.REACT_APP_CLIENT\r\n      },\r\n      cancelToken: this.source.token\r\n    })\r\n      .then((res) => {\r\n        return res;\r\n      })\r\n      .catch((err) => {\r\n        // console.log(err);\r\n      });\r\n\r\n    if ((typeof data) !== 'undefined' && (typeof govres) !== 'undefined') {\r\n      let stats_response;\r\n      stats_response = data.data;\r\n\r\n      let govdata = govres.data;\r\n      Object.keys(govdata).forEach(function (key) {\r\n        if (govdata[key].ObectType === 2) {\r\n          delete govdata[key];\r\n        }\r\n      });\r\n\r\n      var budgetSum = govdata.reduce(function (sum, d) {\r\n        if (d.payment_amount === undefined) {\r\n          return sum + 0;\r\n        } else {\r\n          return sum + d.payment_amount;\r\n        }\r\n      }, 0);\r\n\r\n      if (this._isMounted) {\r\n        this.setState({\r\n          statsData: stats_response,\r\n          govData: govdata,\r\n          budgetSum: budgetSum,\r\n          dataLoad: 1\r\n        });\r\n      }\r\n    } else {\r\n      if (this._isMounted) {\r\n        this.setState({\r\n          dataLoad: 2\r\n        });\r\n      }\r\n    }\r\n\r\n  }\r\n\r\n  render() {\r\n    const {t} = this.props;\r\n    if (this.state.dataLoad === 1) {\r\n\r\n      return (\r\n        <Background>\r\n          <BackgroundInner/>\r\n          <main className=\"section govPage\">\r\n            <MetaTags>\r\n              <title>{t('governance.title')}</title>\r\n              <meta name=\"keywords\" content={t('governance.keywords')}/>\r\n              {/* <meta name=\"description\" content={t('governance.description')} /> */}\r\n            </MetaTags>\r\n            <div className=\"shell-large\">\r\n              <div className=\"section__body\">\r\n                <div className=\"articles\">\r\n                  <BannerImage heading={t('governance.heading')} direction=\"top-right\">\r\n                    <p>{t('governance.par1')}</p>\r\n                    <p>{t('governance.par2')}</p>\r\n                  </BannerImage>\r\n\r\n                  <section className=\"article article--revirse article--offsets-bottom\">\r\n                    <div className=\"cols\">\r\n                      <div className=\"col col--size12\">\r\n                        <div className=\"article__content\">\r\n                          <ProposalsList statsData={this.state.statsData.stats.mn_stats}/>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </section>\r\n\r\n                  <section className=\"article\">\r\n                    <GovDetails budgetSum={this.state.budgetSum}\r\n                                superBlockData={this.state.statsData.stats.superblock_stats}/>\r\n                  </section>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </main>\r\n        </Background>\r\n      )\r\n    } else {\r\n      return (\r\n        <Background>\r\n          <BackgroundInner/>\r\n          <main className=\"section govPage\">\r\n            <MetaTags>\r\n              <title>{t('governance.title')}</title>\r\n              <meta name=\"keywords\" content={t('governance.keywords')}/>\r\n              <meta name=\"description\" content={t('governance.description')}/>\r\n            </MetaTags>\r\n            <div className=\"shell-large\">\r\n              <div className=\"section__body\">\r\n                <div className=\"articles\">\r\n                  <BannerImage heading={t('governance.heading')} direction=\"top-right\">\r\n                    <p>{t('governance.par1')}</p>\r\n                    <p>{t('governance.par2')}</p>\r\n                  </BannerImage>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </main>\r\n        </Background>\r\n      )\r\n    }\r\n  }\r\n}\r\n\r\nexport default withTranslation()(Governance);\r\n","C:\\Users\\AMIR\\Documents\\GitHub\\syshub\\src\\shared\\locales\\resources.js",[],"C:\\Users\\AMIR\\Documents\\GitHub\\syshub\\src\\components\\global\\PrivateRoute.jsx",[],"C:\\Users\\AMIR\\Documents\\GitHub\\syshub\\src\\components\\global\\PublicRoute.jsx",[],"C:\\Users\\AMIR\\Documents\\GitHub\\syshub\\src\\pages\\Recover.jsx",[],"C:\\Users\\AMIR\\Documents\\GitHub\\syshub\\src\\pages\\Login.jsx",[],"C:\\Users\\AMIR\\Documents\\GitHub\\syshub\\src\\pages\\Signup.jsx",[],"C:\\U